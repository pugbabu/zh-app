<view class="tuzhi-wrapper"><view class="cu-bar bg-white search"><view class="search-form round"><text class="cuIcon-search"></text><input type="text" placeholder="请输入图纸名称" confirm-type="search"/></view><view class="action"><button data-event-opts="{{[['tap',[['search',['$event']]]]]}}" class="cu-btn bg-gradual-green shadow-blur round" bindtap="__e">搜索</button></view></view><nav-bar vue-id="1" navs="{{navs}}" data-event-opts="{{[['^tabChange',[['tabChange']]]]}}" bind:tabChange="__e" bind:__l="__l"></nav-bar><view class="tuzhi-list"><scroll-view class="scroll-Y" style="height:calc(100vh - 200rpx);padding:40rpx 0;" scroll-top="{{scrollTop}}" scroll-y="true" data-event-opts="{{[['scrolltoupper',[['upper',['$event']]]],['scrolltolower',[['lower',['$event']]]],['scroll',[['scroll',['$event']]]]]}}" bindscrolltoupper="__e" bindscrolltolower="__e" bindscroll="__e"><block wx:for="{{list}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="tuzhi-item"><view class="tuzhi-title">{{item.title}}</view><image src="{{item.image}}" data-event-opts="{{[['tap',[['toDetail',['$0'],[[['list','',index,'image']]]]]]]}}" bindtap="__e"></image></view></block></scroll-view></view><image hidden="{{!(old.scrollTop>300)}}" class="link-top" src="../../static/knowledge/top.png" data-event-opts="{{[['tap',[['goTop',['$event']]]]]}}" bindtap="__e"></image></view>