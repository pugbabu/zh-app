
.success-wrapper{
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	-webkit-box-orient: vertical;
	-webkit-box-direction: normal;
	-webkit-flex-direction: column;
	    -ms-flex-direction: column;
	        flex-direction: column;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	    -ms-flex-pack: center;
	        justify-content: center;
	padding-top: 200rpx;
}
.success-icon{
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	    -ms-flex-pack: center;
	        justify-content: center;
}
.success-icon image{
	width: 100rpx;
	height: 100rpx;
}
.success-title{
	font-size: 16px;
	color: #5886fc;
	line-height: 20px;
	margin-top: 40rpx;
	text-align: center;
}
.success-tip{
	font-size: 14px;
	color: #666;
	text-align: center;
	margin-top: 20rpx;
}

